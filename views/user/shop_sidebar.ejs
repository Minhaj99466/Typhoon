    <!--sw header wrapper start-->
    <%- include('../userLayout/nav.ejs') %>
      <!--sw header wrapper end-->

      <!-- page_title_section start -->
      <div class="page_title_section">
        <div class="page_header">
          <div class="container">
            <div class="row">
              <!-- section_heading start -->
              <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12">
                <div class="sub_title_section">
                  <ul class="sub_title">
                    <li><a href="/"> Home </a></li>
                    <li>shop</li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- blog_section start -->
      <div class="blog_section blog_shop_wrapper" >
        <div >
          <div class="row" style="display: flex; justify-content: center;">
            <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12">
              <div class="row">
                <div class="sidebar_shop_right">
                  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="filter-area">
                      <div class="list-grid">
                        <ul class="list-inline nav nav-pills">
                          <li class="active_pagination active_shop">
                            <a data-toggle="pill" href="#grid" class="waves-effect waves-light waves-ripple"><i
                                class="flaticon-menu"></i></a>
                          </li>
                          <li>
                            <a data-toggle="pill" href="#list" class="waves-effect waves-light waves-ripple"><i
                                class="flaticon-list"></i></a>
                          </li>
                        </ul>
                      </div>
                      <div class="showpro">
                        <p>Showing 1-9 of 256 Results</p>
                      </div>
                      <select>
                        <option>sort by</option>
                        <option>Laptop</option>
                        <option>usb cable</option>
                        <option>cpu</option>
                      </select>
                    </div>
                  </div>
                  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="tab-content btc_shop_index_content_tabs_main">
                      <div id="grid" class="tab-pane fade in active">
                        <div class="row">
                          <% if(productData.length > 0){
                            for(let i = 0; i< productData.length;i++ ){ %>
                          <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="sw_product_box_wrapper sw_shop_sider_pder">
                              <a href="/singleProduct/<%= productData[i]._id%>">
                              <div class="sw_product_indx_img_wrapper">
                                <img src="/distributorAssets/distributorImage/<%= productData[i].image[0] %>" alt="shop_img" class="img-responsive" />
                              </div>
                              </a>
                              <div class="sw_product_indx_img_cont_wrapper sw_product_indx_img_cont_wrapper_2">
                                <div class="sw_pdct_img">
                                  <img src="/userAssets/images/line.png" alt="img" />
                                </div>
                                <h5>$ <%= productData[i].price %></h5>
                                <h1><a href="javascript:;"><%= productData[i].productName %></a></h1>

                                <div class="cc_li_cont_wrapper">
                                  <i class="fa fa-star"></i>
                                  <i class="fa fa-star"></i>
                                  <i class="fa fa-star"></i>
                                  <i class="fa fa-star-o"></i>
                                  <i class="fa fa-star-o"></i>
                                </div>
                                <div class="cc_li_img_overlay">
                                  <div class="cc_li_img_text">
                                    <ul>
                                      <li>
                                        <a href="javascript:;"><i class="fa fa-shopping-cart"></i></a>
                                      </li>
                                      <li>
                                        <a href="javascript:;"><i class="fa fa-heart"></i></a>
                                      </li>

                                      <li>
                                        <a href="javascript:;"><i class="fa fa-expand"></i></a>
                                      </li>
                                    </ul>
                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>
                          <% }
                                } %>      
                        </div>
                      </div>
                      <div id="list" class="tab-pane fade">
                        <div class="row">
                          <% if(productData.length > 0){
                            for(let i = 0; i< productData.length;i++ ){ %>
                          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="btc_shop_indx_cont_list_box_wrapper">
                              <div class="btc_shop_list_img_wrapper">
                                <img src="/distributorAssets/distributorImage/<%= productData[i].image[0] %>" alt="shop_img" class="img-responsive" />
                              </div>
                              <div class="btc_shop_list_img_cont_wrapper">
                                <div class="btc_shop_product_price_wrapper pdt">
                                  <ul>
                                    <li>$ <%= productData[i].price %></li>
                                  </ul>
                                </div>

                                <h1>
                                  <a href="javascript:;"><%= productData[i].productName %></a>
                                </h1>
                                <div class="cc_shop_list">
                                  <i class="fa fa-star"></i>
                                  <i class="fa fa-star"></i>
                                  <i class="fa fa-star"></i>
                                  <i class="fa fa-star-o"></i>
                                  <i class="fa fa-star-o"></i>
                                </div>

                                <p>
                                  <%= productData[i].description %>
                                </p>

                                <div class="btc_shop_list_bottom_social_wrapper">
                                  <ul>
                                    <li class="shop_list_first_btn">
                                      <a href="#!" class="waves-effect waves-light waves-ripple"><i
                                          class="fa fa-heart"></i>wishlist</a>
                                    </li>
                                    <li>
                                      <a href="#!" class="waves-effect waves-light waves-ripple"><i
                                          class="fa fa-shopping-cart"></i>add to
                                        cart</a>
                                    </li>
                                  </ul>
                                </div>
                              </div>
                            </div>
                          </div>
                          <% }
                                } %>   
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <!-- blog_pagination_section start -->
                    <div class="pager_wrapper gc_blog_pagination">
                      <ul class="pagination">
                        <li><a href="javascript:;">Previous</a></li>
                        <li><a href="javascript:;">1</a></li>
                        <li class="third_pagger"><a href="javascript:;">2</a></li>
                        <li><a href="javascript:;">3</a></li>
                        <li class="hidden-xs"><a href="javascript:;">4</a></li>
                        <li><a href="javascript:;">Next</a></li>
                      </ul>
                    </div>
                    <!-- blog_pagination_section end -->
                  </div>
                </div>
              </div>
            </div>
            
          </div>
        </div>
      </div>
      <!-- blog_section end -->
