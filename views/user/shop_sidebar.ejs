        <!--js code-->
        <script>
          $("#range-price").slider({
            range: true,
            min: 0,
            max: 1200,
            values: [100, 875],
            slide: function (event, ui) {
              $("#price").val("$" + ui.values[0] + " - " + " $" + ui.values[1]);
            },
          });

          $("#price").val(
            "$" +
            $("#range-price").slider("values", 0) +
            " - " +
            " $" +
            $("#range-price").slider("values", 1)
          );

          $("#range-price_2").slider({
            range: true,
            min: 0,
            max: 1200,
            values: [100, 875],
            slide: function (event, ui) {
              $("#price_2").val("$" + ui.values[0] + " - " + " $" + ui.values[1]);
            },
          });

          $("#price_2").val(
            "$" +
            $("#range-price_2").slider("values", 0) +
            " - " +
            " $" +
            $("#range-price_2").slider("values", 1)
          );
        </script>
