      <div class="checkout-page">
        <div class="container">


          <!--Checkout Details-->
          <div class="checkout-form">
            <form action="/place">
              <div class="row clearfix">
                <!--Column-->
                <div class="column col-lg-6 col-md-6 col-sm-12 col-xs-12">
                  <div class="btc_shop_single_prod_right_section related_pdt_shop_head checkout_heading">
                    <h1>BILLING DETAILS</h1>
                  </div>
                  <div class="row clearfix">
                    <!--Form Group-->
                    <div class="form-group col-md-6 col-sm-6 col-xs-12">
                      <div class="field-label">First Name <sup>*</sup></div>
                      <input type="text" name="field-name" value="" placeholder="" required/>
                    </div>

                    <!--Form Group-->
                    <div class="form-group col-md-6 col-sm-6 col-xs-12">
                      <div class="field-label">Phone</div>
                      <input type="text" name="field-name" value="" placeholder="" required/>
                    </div>



                    <!--Form Group-->
                    <div class="form-group col-md-6 col-sm-6 col-xs-12">
                      <div class="field-label">Email Address</div>
                      <input type="text" name="field-name" value="" placeholder="" required/>
                    </div>

                    <!--Form Group-->
                    <div class="form-group col-md-6 col-sm-12 col-xs-12 pr-0">
                      <div class="field-label">Country</div>
                      <select required>
                        <option>Pakistan</option>
                        <option>India</option>
                        <option>Australia</option>
                        <option>Usa</option>
                      </select>
                    </div>



                    <!--Form Group-->
                    <div class="form-group col-md-12 col-sm-12 col-xs-12">
                      <div class="field-label">Address</div>
                      <input type="text" name="field-name" value="" placeholder="Street address" required/>
                    </div>

                    <!--Form Group-->
                    <div class="form-group col-md-6 col-sm-12 col-xs-12 ">
                      <div class="field-label">Town/City</div>
                      <input type="text" name="field-name" value="" placeholder="" required/>
                    </div>
                    <!--Form Group-->
                    <div class="form-group col-md-6 col-sm-12 col-xs-12 ">
                      <div class="field-label">Postcode/ ZIP</div>
                      <input type="text" name="field-name" value="" placeholder="" required/>
                    </div>
                    <!--Form Group-->
                  </div>
                </div>
                <!--Column-->
                <div class="column col-md-6 col-sm-12 col-xs-12 checkout_form_2_wrapper">
                  <!--Order Box-->
                  <div class="btc_shop_single_prod_right_section related_pdt_shop_head checkout_heading">
                    <h1>CHECK OUT PAYEMENT DETAILS</h1>
                    <div class="order-box" style="margin-top: 50px;">
                      <div class="title-box">
                        <div class="col">PRODUCT</div>
                        <div class="col">TOTAL</div>
                      </div>
                      <ul>
                        <li class="clearfix">
                          <strong>Patient Ninja 1</strong><span>$35.00</span>
                        </li>
                        <li class="clearfix">Sub Total<span>$35.00</span></li>
                        <li class="clearfix">
                          Shipping<span class="free">Free Shipping</span>
                        </li>
                        <li class="clearfix">TOTAL<span>$35.00</span></li>
                      </ul>
                    </div>


                    <!--End Order Box-->

                    <!--Payment Box-->
                    <div class="payment-box" style=" display: flex; justify-content: center;">

                      <!--Payment Options-->

                      <div class="payment-options">
                        <div class="btc_shop_single_prod_right_section related_pdt_shop_head checkout_heading">
                          <h1> PAYEMENT</h1>
                        </div>
                        <ul>

                          <li>
                            <div class="radio-option">
                              <input type="radio" name="payment-group" id="payment-1" required />
                              <label for="payment-1"><strong>Check Payments</strong></label>
                            </div>
                          </li>

                          <li>
                            <div class="radio-option">
                              <input type="radio" name="payment-group" id="payment-3" required />
                              <label for="payment-3"><strong>Cash on Delivery</strong></label>
                            </div>
                          </li>
                          <li>
                            <div class="radio-option">
                              <input type="radio" name="payment-group" id="payment-4" required />
                              <label for="payment-4"><strong>PayPal</strong></label>

                            </div>
                          </li>
                          <li>
                            <div class="lower-box text-right">
                              <div class="shop_btn_wrapper checkout_btn">
                                <ul>
                                  <button class="btn btn-succes" type="submit"> Place Order</button>
                                </ul>
                              </div>
                            </div>
                          </li>
                        </ul>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </form>
            <!--End Checkout Details-->




