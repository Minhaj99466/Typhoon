<style>

  .require{
    border: 1px solid green !important;
    background-color: rgb(235, 237, 232) !important;
  }
  .form-btn{
    background-color:green !important ;
  }
  input[placeholder], [placeholder], *[placeholder] {
    color: rgb(0, 0, 0) !important;
}

</style>
<body>
  <div class="login_main_wrapper res_main_wrapper">
    <div class="container-fluid p-0">
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-12 col-12">
          <div class="image-wrap">
            <img src="/userAssets/images/solar2.jpg" alt="img" />
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-12">
          <div class="sw_contact_form_wrapper sw_contact_form_padder login_form_wrapper">
            <div class="contact_right_wrapper">
              <div class="cmnt_area_div_mn">
                <div class="sw_left_heading_wraper sw_dark_heading_wraper">
                  <h1 style="color: green !important; ">Distributor Register</h1>
                  <img src="/userAssets/images/heading_line.png" alt="title" />
                </div>
                <div class="row">
                  <form action="/distributor/register" id="form" method="post">
                    <div class="cont_main_section">
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="contect_form1">
                          <input type="text" id="distributer_name" name="distributer_name" placeholder="Distributer name*" class="require" />
                          <span id="name_error" class="error"></span>
                        </div>
                      </div>
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="contect_form1">
                          <input type="text" id="licence_number" name="licence_number" placeholder="Licence number*" class="require" />
                          <span id="adhar_error" class="error"></span>
                        </div>
                      </div>
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="contect_form1">
                          <input type="text" id="phonenumber" name="phonenumber" placeholder="Phone number*" class="require" />
                          <span id="phone_error" class="error"></span>
                        </div> 
                      </div>
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="contect_form1">
                          <input type="email" id="email" name="email" placeholder="Email*" class="require" data-valid="email"
                            data-error="Email should be valid." />
                            <span id="email_error" class="error"></span>
                        </div>
                      </div>
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="contect_form1">
                          <input type="password" id="password" name="password" placeholder="Password*" class="require"
                            data-valid="email" data-error="Email should be valid." />
                            <span id="pass_error" class="error"></span>
                        </div>
                      </div>
                      <% if(typeof message !=='undefined' ){ %>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                          <p class="Danger" role="alert">
                            <%= message %>
                          </p>
                        </div>
                        <% } %>
                          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="buttons">
                              <button type="submit" class="form-btn">
                                Sign up
                              </button>
                            </div>
                          </div>
                          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="login-sec">
                              <span>Already have an account ? </span>
                              <a href="/distributor"> Login Now</a>
                            </div>
                          </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <script src="/userAssets/js/jquery_min.js"></script>
  <script src="/userAssets/js/materialize.min.js"></script>
  <script src="/userAssets/js/bootstrap.min.js"></script>
  <script src="/userAssets/js/jqu_menu.js"></script>
  <script src="/userAssets/js/jqu_slickmenu.js"></script>
  <script src="/userAssets/js/isotope.pkgd.min.js"></script>
  <script src="/userAssets/venobox/js/venobox.min.js"></script>
  <script src="/userAssets/js/jquery.inview.min.js"></script>
  <script src="/userAssets/js/jquery.mixitup.min.js"></script>
  <script src="/userAssets/js/jquery.countTo.js"></script>
  <script src="/userAssets/js/wow.min.js"></script>
  <script src="/userAssets/js/jquery.easing.1.3.js"></script>
  <script src="/userAssets/js/owl.carousel.js"></script>
  <script src="/userAssets/js/camera.min.js"></script>
  <script src="/userAssets/js/custom_2.js"></script>
